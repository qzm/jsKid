    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "jskid",
    "physics",
    "matter-js",
    "game-engine",
    "2d",
    "collision"
  ],
  "author": "QZM <qzmdev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@jskid/core": "workspace:*",
    "@jskid/utils": "workspace:*",
    "matter-js": "^0.19.0",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/matter-js": "^0.19.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "@jskid/core": "^2.0.0"
  }
