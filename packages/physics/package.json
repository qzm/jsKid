{
  "name": "@jskid/physics",
  "version": "2.0.0",
  "description": "Physics engine integration for jsKid 2D game engine - Matter.js wrapper",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
